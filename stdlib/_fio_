DEF readFile(f) DO
	RETURN shell("cat " + f)
ENDEF
DEF writeFile(f,data) DO
	RETURN shell("echo '"+data+"' >| " +f)
ENDEF
DEF appendFile(f,data) DO
	RETURN shell("echo '"+data+"' >> "+f)
ENDEF
DEF removeFile(f) DO
	RETURN shell("rm " + f)
ENDEF

DEF listDir(d) DO
	list <- shell("ls")
	

