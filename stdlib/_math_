PI <- 3.141592653589793
DEF sqrt(a) DO
	RETURN a ^ 0.5
ENDEF
DEF factorial(a) DO
	b <- 1
	FOR i <- 1 TO a
		b <- b * a
	NEXT i + 1
	RETURN b
ENDEF
DEF sin(a) DO
	RETURN a - (a^3)/factorial(3) + (a^5)/factorial(5) - (a^7)/factorial(7) + (a^11)/factorial(11) - (a^13)/factorial(13) + (a^15)/factorial(15) - (a^17)/factorial(17) + (a^19)/factorial(19)
ENDEF
DEF cos(a) DO
	RETURN sin(90-a)
ENDEF
DEF tan(a) DO
	RETURN sqrt(1-cos(2*a)/1+cos(2*a))
ENDEF
DEF deg(a) DO
	RETURN a * 180 / PI
ENDEF
DEF rad(a) DO
	RETURN a * PI/180
ENDEF